#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int X, Y, Z;
    cin >> X >> Y >> Z;
    if (X > Z) {
        cout << 0 << endl;
    } else if (X + Y <= Z) {
        cout << 2 << endl;
    } else {
        cout << 1 << endl;
    }
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    return 0;
}
